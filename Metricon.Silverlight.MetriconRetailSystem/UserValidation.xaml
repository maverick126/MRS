<navigation:Page x:Class="Metricon.Silverlight.MetriconRetailSystem.UserValidation" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
           d:DesignWidth="640" d:DesignHeight="480"
           Title="UserValidation Page">
    <telerik:RadBusyIndicator x:Name="BusyIndicator1" BusyContent="Validating Account....">
        <Grid x:Name="LayoutRoot">
            <TextBlock Name="txtValidationFailed" Visibility="Collapsed">Invalid User</TextBlock>
        </Grid>
    </telerik:RadBusyIndicator>
</navigation:Page>
